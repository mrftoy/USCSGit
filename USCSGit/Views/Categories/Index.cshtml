@model IEnumerable<USCS.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Domain)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrimaryCategory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondaryCategory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TertiaryCategory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Eff_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Thru_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Deleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Load_Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Domain)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrimaryCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondaryCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TertiaryCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Eff_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Thru_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Load_Date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CategoryID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CategoryID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CategoryID })
        </td>
    </tr>
}

</table>

